%div
  %h1 Update Venue
  = form_for @venue, url: admin_venue_path(@venue) do |f|
    %div
      = f.label :name
      = f.text_field :name
    %div
      = f.label :address_1
      = f.text_field :address_1
    %div
      = f.label :city
      = f.text_field :city
    %div
      = f.label :state
      = f.text_field :state
    %div
      = f.label :zip_code
      = f.text_field :zip_code
    %div
      = f.submit