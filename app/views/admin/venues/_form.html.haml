= bootstrap_form_for [:admin, @venue] do |f|
  %fieldset
    = f.text_field :name
    = f.text_field :address_1
    = f.text_field :city
    = f.text_field :state
    = f.text_field :zip_code
    = f.text_field :phone_number
    = f.text_field :web_address
    = f.text_field :map_link
    = f.submit 'Submit', class: 'btn btn-primary'